'
' 	For loop - note it is closer to Pascal now
'	To/DownTo replaces step. No index on next.
'
for i = 1 to 8:print i;" ";:next:print
for i = 12 downto 8:print i;" ";:next:print
print
'
'	Repeat loop
'
i = 0
repeat
	print i;" ";:i = i + 1
until i = 10
print:print
'
'	While loop
'
i = 0
while i <  10
	print i;" ";:i = i + 1
wend
print:print
'
'	General Do Loop with Exit
'
i = 0
do 
	i = i + 1:print i;" ";
	if i = 10 then exit
loop
print:print